    "keywords": ["Ubisoft", "Nadeo", "ManiaPlanet", "ShootMania", "TrackMania", "Manialib"],
    "license": "MIT",
    "homepage": "https://github.com/manialib",
    "authors": [
        {
            "name": "Nadeo",
            "email": "contact@nadeo.com",
            "homepage": "http://www.nadeo.com/"
        }
